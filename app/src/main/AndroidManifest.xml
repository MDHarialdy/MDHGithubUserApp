<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MDHGithubUserApp"
        tools:targetApi="31" >
        <activity
            android:name="com.belajar.mdh.githubuserapp.ui.darkmode.SettingActivity"
            android:exported="false"
            android:label="@string/settings"/>
        <activity
            android:name="com.belajar.mdh.githubuserapp.ui.favorite.FavoriteUserActivity"
            android:exported="false"
            android:label="@string/favorite"/>
        <activity
            android:name="com.belajar.mdh.githubuserapp.ui.detail.DetailActivity"
            android:exported="false"
            android:label="@string/Detail_Activity" />
        <activity
            android:name="com.belajar.mdh.githubuserapp.ui.main.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>